{
  "name": "eidpng",
  "version": "4.0.0",
  "description": "Angular 2 application for EIDP4",
  "main": "index.html",
  "scripts": {
    "start": "npm run httpserver",
    "tsc": "tsc -p app/",
    "tsc:w": "tsc -p app/ -w",
    "httpserver": "http-server -c-1",
    "postinstall": "tsc",
    "snyk-test": "snyk test",
    "test": "snyk test && tsc && karma start karma.conf.js",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UCL/EIDP-4.git"
  },
  "keywords": [
    "angular2"
  ],
  "author": "David Guzman",
  "contributors": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UCL/EIDP-4/issues"
  },
  "homepage": "https://github.com/UCL/EIDP-4#READNE.md",
  "dependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "ng2-bootstrap": "^1.6.1",
    "reflect-metadata": "^0.1.9",
